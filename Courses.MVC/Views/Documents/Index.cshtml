    <div class="col-md-7">
        <div class="row">
            <div class="col">
                <h2 style="color:White;">List of Images</h2>
            </div>           
        </div>
        <div class="row">
            <table>
                <thead>
                    <tr>
                        <th style="color:white;">Image Name</th>&nbsp;
                        &nbsp;
                        &nbsp;

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td style="color:white;">@item.ImageName</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
